package Basicjava;

public class StringInJava {
	
	public static void main(String[] args) {
		
		String Str="java";
		String str1="PYTHON";
		
		System.out.println(Str+"is complex than : "+str1); // String concatination;
		System.out.println(Str.toUpperCase()); // convert in upper class
		System.out.println(str1.toLowerCase()); // convert upper to lower 
		
		
	}

}
