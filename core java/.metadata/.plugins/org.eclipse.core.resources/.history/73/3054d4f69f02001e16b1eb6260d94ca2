package Basicjava;

public class JavaMath {
	public static void main(String[] args) {
		int var1= 10;
		int var2= 20;
		
		int max =Math.max(var1, var2); // get maximum number between them
		System.out.println(max);
		
		int min= Math.min(var2, var1); // get minimum number between them 
		System.out.println(min);
		
		System.out.println(Math.sqrt(var1)); // for calculate the Square root.
		
		System.out.println(Math.random());
		
		
	}

}
